<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sales.biomed">
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="sales.biomed.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="sales.biomed.home" />
        <activity android:name="sales.biomed.Subhome" />
        <activity android:name="sales.biomed.Profile" />
        <activity android:name="sales.biomed.Orders"></activity>
        <activity android:name="sales.biomed.DailyWork"></activity>
        <activity android:name="sales.biomed.Expenses"></activity>
        <activity android:name="sales.biomed.Receipt"></activity>
        <activity android:name="sales.biomed.Complaints"></activity>
    </application>

</manifest>